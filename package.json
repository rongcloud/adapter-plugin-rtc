{
  "name": "@rongcloud/adapter-plugin-rtc",
  "version": "1.0.8-alpha.5",
  "description": "@rongcloud/adapter-plugin-rtc",
  "private": true,
  "main": "./dist/index.js",
  "__attrs__": {
    "globals": "RCRTCAdapter"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "npm run lint && rollup -c scripts/rollup.prod.js",
    "build:dev": "npm run lint && rollup -c scripts/rollup.dev.js",
    "release": "node scripts/release.prod",
    "dev": "node scripts/release.dev && cd release/npm && npm link",
    "lint": "eslint --ext .js,.ts .",
    "fix": "npm run lint -- --fix",
    "upload-cdn": "node scripts/upload-cdn.js"
  },
  "keywords": [
    "RongCloud",
    "JSSDK",
    "WebRTC",
    "RTCLib"
  ],
  "contributors": [
    "kongcong@rongcloud.cn",
    "lizhipeng@rongcloud.cn",
    "lishunan1@rongcloud.cn"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "license": "LGPL 2.1",
  "homepage": "https://www.rongcloud.cn",
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "*.[jt]s": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "core-js": "^3.8.1",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "rollup": "^2.35.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3",
    "validate-commit-msg": "^2.14.0",
    "qiniu": "^7.3.3"
  },
  "peerDependencies": {
    "@rongcloud/engine": "^4.4.8-alpha.1",
    "@rongcloud/plugin-rtc": "^5.1.8-alpha.3"
  },
  "dependencies": {
    "@rongcloud/engine": "^4.4.8-alpha.1",
    "@rongcloud/plugin-rtc": "^5.1.8-alpha.3"
  }
}
